<%= render 'shared/errors', obj: @property %>
<%= form_with(model: [ @category, @property ]) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, id: :property_name %>

  <%= f.label :value %>
  <%= f.text_field :value, id: :property_value %>

  <%= f.label :suffix %>
  <%= f.text_field :suffix, id: :property_suffix %>

  <%= f.submit %>
<% end %>
