<%= render 'shared/errors', obj: @property %>
<%= form_with(model: [ @category, @property ]) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, id: :property_name %>

  <%= f.label :pixel_value %>
  <%= f.number_field :pixel_value, id: :property_pixel_value %>

  <%= f.label :hex_value %>
  <%= f.text_field :hex_value, id: :property_hex_value %>

  <%= f.submit %>
<% end %>
